#!/bin/bash
source /home/user/data.txt
docker pull schnitzler/mysqldump:3.4
docker run \
    --network shvirtd-example-python_backend \
    --rm --entrypoint "" \
    -v `pwd`/backup:/backup \
    --link="db" \
    schnitzler/mysqldump \
    mysqldump -uroot --result-file=/backup/dumps.sql -p --all-databases
